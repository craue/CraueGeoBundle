{
	"name": "craue/geo-bundle",
	"type": "symfony-bundle",
	"description": "Doctrine functions for calculating geographical distances in your Symfony2 project.",
	"keywords": ["symfony2", "geo"],
	"homepage": "https://github.com/craue/CraueGeoBundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Raue",
			"email": "christian.raue@gmail.com"
		},
		{
			"name": "Symfony Community",
			"homepage": "https://github.com/craue/CraueGeoBundle/contributors"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"symfony/framework-bundle": "~2.0"
	},
	"require-dev": {
		"doctrine/orm": "~2.3",
		"doctrine/data-fixtures": "~1.0",
		"doctrine/doctrine-bundle": "~1.0",
		"phpunit/phpunit": "~4.0",
		"symfony/symfony": "~2.0"
	},
	"suggest": {
		"doctrine/data-fixtures": "*"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"autoload": {
		"psr-0": {
			"Craue\\GeoBundle\\": ""
		}
	},
	"target-dir": "Craue/GeoBundle"
}
